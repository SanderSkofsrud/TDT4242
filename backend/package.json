  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npx ts-node-dev --respawn --transpile-only server.ts",
    "start": "node dist/server.js",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings 0",
